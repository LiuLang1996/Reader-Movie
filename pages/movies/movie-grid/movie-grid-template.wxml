<import src="../movie/movie-template.wxml" />

<template name="movieGrid">
  <view class="grid-container">
    <block wx:for="{{movies}}" wx:for-item="movie" wx:key="movie">
      <view class="single-view-container">
        <template is="movie" data="{{...movie}}" />
      </view>
    </block>
  </view>
</template>